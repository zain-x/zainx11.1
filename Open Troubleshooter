@echo off
title Open Troubleshooter
color f0

REM Run this in Administrator Mode

:home
cls
set /p tc=Troubleshooting Code:
if %tc% == 6527 goto tc1
goto home

:tc1
cls
del %USERPROFILE%\Desktop\ZWord-Open.bat
echo @echo offtitle ZWord Opencolor f0:homeclsecho 1 Newecho 2 Exitset /p n=if %n% == 1 goto 1if %n% == 2 Exit:1clsset /p e=Name:clsecho 1 Desktopecho 2 Documentsecho 3 Customset /p l=if %l% == 1 goto 1aif %l% == 2 goto 1bif %l% == 3 goto c:1aset q=Desktopclsgoto m:1bset q=Documentscls:mset /p t=echo %t% > %USERPROFILE%\%q%goto m:cclsset /p q=Where? if exist %USERPROFILE%\%l% goto mclsgoto c >> %USERPROFILE%\Desktop\ZWord-Open.bat
goto home
